{
  "bin": "bin/winser.js",
  "pkg": {
    "scripts": [
      "app.js",
      "bin/*.js",
      "routes/**/*.js"
    ],
    "assets": [
      "package.json",
      "setup/setup.bat",
      "bin/daemon/*",
      "public/**/*",
      "views/**/*"
    ]
  },
  "name": "avi-player-ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www.js",
    "dev_start": "nodemon ./bin/www.js",
    "install_service": "node ./bin/install.js",
    "uninstall_service": "node ./bin/uninstall.js",
    "test_cmd": "node ./bin/winser.js",
    "pkg": "pkg -t win package.json --output setup/avi-player-ui.exe",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress "
  },
  "dependencies": {
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "^2.6.1",
    "element-ui": "^2.4.6",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "node-cmd": "^3.0.0",
    "node-xlsx": "^0.12.1",
    "sqlite3": "^4.0.2",
    "vue": "^2.3.4",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "license": "MIT"
}
